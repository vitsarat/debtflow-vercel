{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dashboard.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    }
  ]
}